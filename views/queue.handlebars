<div class="container">
  <div class="row">
    <div class="col-8 offset-2">
      <div class="card mt-4">
        <div class="card-body">
          <h5 class="card-title">Current Queue</h5>
          <ul id="queue-list" class="list-group">
          {{#each Queues}}
            <li data-id="{{this.id}}" class="list-group-item">
              {{this.id}}. {{this.name}} - {{this.queue_state}}
              <p><strong>{{this.Song.song}}</strong> by <strong>{{this.Song.artist}}</strong>, Genre: {{this.Song.genre}}, Musical Key: {{this.Song.music_key}}</p>
            </li>
          {{/each}}
        </ul>
            <a href="/">‚Üê Back To Home</a>
        </div>
      </div>
    </div>
  </div>
</div>